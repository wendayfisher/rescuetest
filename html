        function loadInstantChat()
        {
            var ICLoader = new RescueInstantChatLoader();
            ICLoader.ICContainer = "ICContainer";
            ICLoader.ApplicationID = "RescueInstantChat";
           
            
            ICLoader.HostedCSS = 'https://raw.githubusercontent.com/wendayfisher/rescuetest/refs/heads/main/InstantChat.css';
            
            
            ICLoader.HostedLanguagesForChatOnlyMode = 'https://secure.logmeinrescue.com/InstantChat/LanguagesForChatOnlyMode.js?v=16010101000000';
            ICLoader.HostedLanguagesForAppletMode = 'https://secure.logmeinrescue.com/InstantChat/LanguagesForAppletMode.js?v=16010101000000';
            ICLoader.PageTitle = "###COMPANYNAME###";
            ICLoader.PrivateCode = null;
            ICLoader.EntryID = '6032277';
            ICLoader.Name = null;
            ICLoader.Comment1 = null;
            ICLoader.Comment2 = null;
            ICLoader.Comment3 = null;
            ICLoader.Comment4 = null;
            ICLoader.Comment5 = null;
            ICLoader.Tracking0 = null;
            ICLoader.Language = 'en';
            ICLoader.HostedErrorHandler = function (ErrorName) {
            };
            ICLoader.Start();
            ICLoader.centralizeGUI();
        }

        function isInHighContrastMode() {

            let hcDetect = $('<div id="highContrastDetect" style="width: 0px; height: 0px;"></div>').css('background-color', '#ff0000');

            hcDetect.appendTo(document.body);

            var bgColor = hcDetect.css('background-color').toLowerCase();

            if (bgColor !== "#ff0000" && bgColor !== "rgb(255, 0, 0)") {
                return true;
            }
            
            return false;
        }
    </script>

