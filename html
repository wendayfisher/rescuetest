     <script type="text/javascript">

        function iframeSetup() {
            var contentIframe = document.getElementById('ic_mainframe');

            if (!contentIframe) {
                setTimeout(iframeSetup, 200);
            }
            else {
                contentIframe.onload = setHighContrastTheme;
            }
        }

        function setHighContrastTheme() {
            // at this point we are still out of the iframe context so we need to retrive it from the hosting document
            var contentIframe = document.getElementById('ic_mainframe');
            var icDocument = contentIframe.contentDocument || contentIframe.contentWindow.document;

            console.log(icDocument);

            var fileSendButton = icDocument.getElementById('ic_filesend_button');
            fileSendButton.innerHTML = "üìÅ";

            var disconnectButton = icDocument.getElementById('ic_disconnect_button');
            disconnectButton.innerHTML = "‚ùå";

            var chatSendButton = icDocument.getElementById('ic_send_button');
            chatSendButton.innerHTML = '&#9166';
         }

         if (isInHighContrastMode()) {
             setTimeout(iframeSetup, 200);
         }
    </script>
</body>
